/* ~~~~~~~~~~~~~~~~~~~~Hover button CSS~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
/* ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
.IR {
  position: relative;
  overflow: hidden;
  font-size: 1em;
}
 
.IR em {
  display: block;
  position: absolute;
  top: 0; left: 0;
  z-index: 1;
}
 
/*###########################*/
/* Hover button CSS for HOME*/
/*##########################*/

button#IRbutton1 {
  background: none;
  border: none;
  float: left;
  display: inline;
  left: 25px;
}
#IRbutton1:hover { cursor: pointer; /* cursor: hand; for IE5 */ }
#IRbutton1, #IRbutton1 em { width: 300px; height: 188px; }
#IRbutton1 em { background: url(homePanel.png) no-repeat; }
#IRbutton1:hover em, #IRbutton1:focus em { background: url(homePanel.gif); background-position: -300px 0; }
 
/* for ie5.x/mac only */
* html>body .IR {
  position: static;
  overflow: visible;
  font-size: 10px;
}
* html>body .IR em { position: static; }
* html>body #IRbutton1 em { margin-bottom: -26px; }


